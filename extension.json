{
	"name": "Less",
	"author": [
		"Cqm"
	],
	"url": "https://runescape.wiki",
	"descriptionmsg": "less-desc",
	"version": "0.1",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
